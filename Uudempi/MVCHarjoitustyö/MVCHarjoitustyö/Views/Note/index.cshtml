@using Microsoft.AspNet.Identity
@using MVCHarjoitustyö.Models
@model NoteViewModel

@{
    ViewBag.Title = "index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>index</h2>

@{ 
    if (Model.notes != null)
    {
        for (int i = 0; i < Model.notes.Count; i++)
        {
            WriteLiteral("<img src=\"data:image/png;base64," + Convert.ToBase64String(Model.notes[i].ImageContent) + "\">");
        }
    }

}



